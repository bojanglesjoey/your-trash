<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Results">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="266dp"
        android:layout_height="221dp"
        android:layout_below="@+id/imageProgress"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/alert_dark_frame" />

    <TextView
        android:id="@+id/Object_Label"
        android:layout_width="73dp"
        android:layout_height="27dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="237dp"
        android:text="@string/Label_Object"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Label_Location"
        android:layout_width="72dp"
        android:layout_height="29dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="24dp"
        android:text="@string/Label_Location"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Object_Label" />

    <TextView
        android:id="@+id/Result_Label"
        android:layout_width="116dp"
        android:layout_height="28dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="28dp"
        android:text="@string/Label_Disposal"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Label_Location" />

    <TextView
        android:id="@+id/databox_object"
        android:layout_width="120dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:text="@string/databox_object"
        android:textSize="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Object_Label"
        app:layout_constraintStart_toEndOf="@+id/Object_Label" />

    <TextView
        android:id="@+id/databox_location"
        android:layout_width="128dp"
        android:layout_height="29dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        android:textSize="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/Label_Location"
        app:layout_constraintStart_toEndOf="@+id/Label_Location" />

    <TextView
        android:id="@+id/databox_results"
        android:layout_width="264dp"
        android:layout_height="97dp"
        android:layout_marginStart="48dp"
        android:text="TextView"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Result_Label" />

</android.support.constraint.ConstraintLayout>